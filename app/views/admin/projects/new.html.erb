<h1>New project</h1>

<%= form_for([:admin, @project]) do |f| %>
  <p>
    <b>Name</b><br />
    <%= f.text_field :name %>
  </p>

  <p>
    <b>Type</b><br />
    <%= f.text_field :projecttype %>
  </p>

  <p>
    <b>Url1</b><br />
    <%= f.text_field :url1 %>
  </p>

  <p>
    <b>Started</b><br />
    <%= f.text_field :started %>
  </p>

  <p>
    <b>Finished</b><br />
    <%= f.text_field :finished %>
  </p>

  <p>
    <b>Url2</b><br />
    <%= f.text_field :url2 %>
  </p>

  <p>
    <b>Description</b><br />
    <%= f.text_area :description %>
  </p>

  <p>
    <b>Major</b><br />
    <%= f.check_box :major %>
  </p>
  <p>
    <b>Sort order</b><br />
    <%= f.text_field :sortorder %>
  </p>

  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', admin_projects_path %>
